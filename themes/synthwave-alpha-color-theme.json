{
	"name": "Synthwave Alpha",
	"type": "dark",
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"*.decorator": "#7f7094",
		"class.builtin": "#55a8fb",
		"comment": "#7f7094",
		"decorator": "#7f7094",
		"derive": "#aa54f9",
		"enum": "#aa54f9",
		"enumMember": "#c488fb",
		"function": "#55a8fb",
		"interface": "#aa54f9",
		"keyword": "#ff00f6",
		"macro": "#55a8fb",
		"method": "#55a8fb",
		"module.decorator": "#7f7094",
		"module": "#0ae4a4",
		"namespace": "#0ae4a4",
		"number": "#e60a70",
		"operator": "#b9b1bc",
		"parameter": "#f9f972",
		"property": "#fcfcb9",
		"selfParameter": "#88c3fd",
		"selfTypeKeyword": "#88c3fd",
		"string": "#00fbfd",
		"struct": "#aa54f9",
		"type": "#aa54f9",
		"variable.defaultLibrary": "#0ae4a4",
		"variable": "#fcfcb9",
	},
	"tokenColors": [
		// generic
		{ "scope": "comment, punctuation, meta.brace, constant.character, keyword.operator, storage.type.function.arrow", "settings": { "foreground": "#7f7094" }},
		{ "scope": "constant.numeric, keyword.other.unit.hexadecimal", "settings": { "foreground": "#e60a70" } },
		{ "scope": "entity.name.function, meta.function-call, support.function", "settings": { "foreground": "#55a8fb" } },
		{ "scope": "entity.name.type, entity.other.inherited-class, meta.interface, support.type, storage.type.string, storage.type.error, storage.type.boolean", "settings": { "foreground": "#aa54f9" } },
		{ "scope": "entity.name.module, entity.name.type.package", "settings": { "foreground": "#0ae4a4" } },
		{ "scope": "constant.language, meta.var.expr, keyword, keyword.operator.expression, keyword.operator.new, storage.modifier, storage.type.class, storage.type.function, support.constant", "settings": { "foreground": "#ff00f6" } },
		{ "scope": "string, variable.parameter.url", "settings": { "foreground": "#00fbfd" } },		
		{ "scope": "constant.character.escape, string.regexp, punctuation.definition.interpolation, punctuation.definition.string, punctuation.definition.template-expression", "settings": { "foreground": "#00b0b1" } },
		{ "scope": "variable.parameter", "settings": { "foreground": "#f9f972" } },
		{ "scope": "variable, meta.object-literal, variable.object.property, support.type.property-name, source.ignore", "settings": { "foreground": "#fcfcb9" } },
		
		// markup
		{ "scope": "entity.name.section", "settings": { "foreground": "#ff00f6" } },
		{ "scope": "entity.name.tag", "settings": { "foreground": "#aa54f9" } },
		{ "scope": "entity.other.attribute-name", "settings": { "foreground": "#c488fb" } },
		{ "scope": "fenced_code.block.language", "settings": { "foreground": "#7f7094" } },
		{ "scope": "markup.bold", "settings": { "foreground": "#00fbfd", "fontStyle": "bold" } },
		{ "scope": "markup.inline.raw.string", "settings": { "foreground": "#00fbfd" } },
		{ "scope": "markup.italic", "settings": { "foreground": "#00fbfd", "fontStyle": "italic" } },
		{ "scope": "markup.quote", "settings": { "foreground": "#aa54f9" } },
		{ "scope": "markup.strikethrough", "settings": { "foreground": "#00b0b1" } },
		{ "scope": "markup.underline.link", "settings": { "foreground": "#55a8fb" } },
		{ "scope": "punctuation.definition.heading", "settings": { "foreground": "#b300ad" } },
		{ "scope": "support.type.property-name.array, support.type.property-name.table", "settings": { "foreground": "#ff00f6" } },
		
		// language specific
		{ "scope": "meta.tag.xml, meta.tag.preprocessor.xml", "settings": { "foreground": "#7f7094" } },
		{ "scope": "variable.css", "settings": { "foreground": "#f9f972" } },
		{ "scope": "meta.var.exp.tsx, keyword.operator.logical.python", "settings": { "foreground": "#ff00f6" } },
		{ "scope": "support.class.component", "settings": { "fontStyle": "bold" } },
	],
